#include "hardware_module.h"
#define GPIOA_EN   (1U<<0)




int main()
{
	void light_int(){
	//enable clock access to the PA port
	RCC->AHB2ENR  |=GPIOA_EN;
	// set pin pa5 to output
	GPIOA -> MODER |=  (1U<<10);
	GPIOA -> MODER &=~(1U<<11);
	}
	void light_on(void)
	{
		/*Set PA5 output to high*/
		GPIOA->ODR |=(1U<<5);
	}

	void light_off(void)
	{
		/*Set PA5 output to LOW*/
		GPIOA->ODR &=~(1U<<5);
	}




}
