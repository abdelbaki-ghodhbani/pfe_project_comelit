#ifndef ESP82XX_LIB_H_
#define ESP82XX_LIB_H_
#include <stdint.h>

typedef struct {
    char ssid[32];
    char password[64];
    int mode;
} ESP_Config;

void esp82xx_reset(void);
void esp82xx_startup_test(void);
 void esp82xx_sta_mode(ESP_Config *config);
void esp82xx_ap_connect(ESP_Config *config);



#endif /* ESP82XX_LIB_H_ */
