#include <stdio.h>
#include "stm32f4xx.h"
#include "esp82xx_driver.h"
#include "circular_buffer.h"
#include "esp82xx_lib.h"


int main()
{
	ESP_Config esp_config;
	    strcpy(esp_config.ssid, "YourSSID");
	    strcpy(esp_config.password, "YourPassword");
	    esp_config.mode = 1;

	    esp82xx_set_mode(&esp_config);
	    esp82xx_ap_connect(&esp_config);




while(1)
	{

	}

}

